<link href='//fonts.googleapis.com/css?family=Ubuntu+Condensed' rel='stylesheet' type='text/css'>

<!--web-fonts-->
<script src="~/Content/js/dangnhap-1.js"></script>
<script src="~/Content/js/easyResponsiveTabs.js" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#horizontalTab').easyResponsiveTabs({
            type: 'default', //Types: default, vertical, accordion
            width: 'auto', //auto or any width like 600px
            fit: true   // 100% fit in a container
        });
    });
</script>

<div class="main-content-w3">
    <div class="sap_tabs-w3agile">
        <div id="horizontalTab" style="display: block; width: 100%; margin: 0px;">
            <div class="tab">
                <ul>
                    <li class="resp-tab-item" aria-controls="tab_item-0" role="tab"><span>THÔNG TIN NGƯỜI THUÊ</span></li>
            @*<li class="resp-tab-item" aria-controls="tab_item-1" role="tab"><span>ĐĂNG NHẬP</span></li>
            <li class="resp-tab-item" aria-controls="tab_item-2" role="tab"><span>Reset Password</span></li>*@
            </ul>
            </div>
            <div class="tab-1 resp-tab-content" aria-labelledby="tab_item-0">
                <p class="tab-text"></p>
                <div class="register ">

                    @using (Html.BeginForm("DKTS","Home",null,FormMethod.Post,new { id = "MyForm"}))
                    {
                        @Html.AntiForgeryToken()

                    <div class="register-top-grid">


                        <span style="text-align:center">Họ và tên</span>
                        @Html.TextBox("TenKH")
                        <span style="text-align:center">Ngày sinh</span>
                        @Html.TextBox("NgaySinh")
                        <span style="text-align:center">CMND/CCCD</span>
                        @Html.TextBox("CMND")
                        <span style="text-align:center">Địa chỉ</span>
                        @Html.TextBox("DiaChi")
                        <span style="text-align:center">Số điện thoại</span>
                        @Html.TextBox("SDT")
                        <span style="text-align:center">Ngày thuê</span>
                        @Html.TextBox("NgayBD")
                        <span style="text-align:center">Mã Xe</span>
                        @Html.TextBox("MaXe")
                        





                    </div>
                        <div class="sign-up">
                            <input type="submit" value="Đăng Ký Thuê">
                        </div>
                    }




                </div>
            </div>
            @*<div class="tab-2 resp-tab-content" aria-labelledby="tab_item-1">

                    <h4 class="text-center"></h4>
                    <div class="register">
                        <form action="#" method="post">
                            <input placeholder="Tên Tài Khoản" name="Email" class="user" type="text" required="">
